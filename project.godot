[dialogue_manager]

runtime/advanced/uses_dotnet=true
runtime/state_autoload_shortcuts=["NPC", "SFX"]
runtime/balloon_path="uid://r2m5r5ydtdx4"

[display]

window/stretch/mode="canvas_items"
window/vsync/vsync_mode=2

[dotnet]

project/assembly_name="GodotBlenderRPGFramework"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/terrain_3d/plugin.cfg")

